<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingyue.provider.dept.mapper.DeptMapper">
    <select id="selectDeptByUser" resultType="com.jingyue.api.pojo.User">
        select * from user u where u.dept_id=#{deptId}
    </select>
</mapper>